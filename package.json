{
  "name": "orbit-client",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "author": "Motonary",
  "license": "ISC",
  "scripts": {
    "release": "webpack --mode production --config webpack.config.js",
    "watch": "webpack-dev-server --mode development --history-api-fallback",
    "lint": "prettier --check './src/**/*.ts' './src/**/*.tsx'",
    "fix": "prettier --write",
    "fix:all": "prettier --write ./src/javascripts/**/*.{ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --check"
  },
  "jest": {
    "setupFiles": [
      "./src/jest.config.js"
    ],
    "transform": {
      "\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|jpg|jpeg|png|gif)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransformer.js"
    }
  },
  "description": "== README",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "animejs": "^2.2.0",
    "axios": "^0.18.0",
    "formik": "^1.4.1",
    "keymirror": "^0.1.1",
    "moment": "^2.23.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-modal": "^3.6.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-s-alert": "^1.4.1",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-backgrounds": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@types/animejs": "^2.0.1",
    "@types/keymirror": "^0.1.1",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-modal": "^3.6.0",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-s-alert": "^1.3.2",
    "@types/redux-promise": "^0.5.28",
    "@types/storybook__react": "^4.0.2",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^4.2.0",
    "husky": "^1.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.5",
    "prettier": "1.15.2",
    "react-hot-loader": "^4.3.8",
    "react-test-renderer": "^16.5.2",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "directories": {
    "test": "test"
  }
}
