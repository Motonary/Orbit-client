{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "no-console": "error",
    "no-debugger": true,
    "one-variable-per-declaration": false,
    "prettier": true,
    "quotemark": [true, "double", "avoid-escape"],
    "variable-name": false
  }
}
